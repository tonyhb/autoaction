{
  "name": "autoaction",
  "version": "0.0.9",
  "description": "Declarative data loading for redux + react",
  "main": "transpiled/autoaction.js",
  "scripts": {
    "build": "./node_modules/babel-cli/bin/babel.js src --out-dir transpiled",
    "build:dev": "./node_modules/babel-cli/bin/babel.js --watch src --out-dir transpiled",
    "prepublish": "npm run build",
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-register --recursive --require ./test/setup.js",
    "test:dev": "./node_modules/mocha/bin/mocha --compilers js:babel-register --recursive --watch --require ./test/setup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/tonyhb/autoaction.git"
  },
  "keywords": [
    "autoaction",
    "react",
    "redux",
    "data",
    "loading"
  ],
  "author": "Tony Holdstock-Brown",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tonyhb/autoaction/issues"
  },
  "homepage": "https://github.com/tonyhb/autoaction#readme",
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0-0",
    "react-redux": "^4.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "jsdom": "^9.2.1",
    "mocha": "^2.5.3",
    "react-addons-test-utils": "^0.14.3",
    "react-dom": "^0.14.3",
    "sinon": "^1.17.4"
  },
  "dependencies": {
    "deep-equal": "^1.0.1"
  }
}
